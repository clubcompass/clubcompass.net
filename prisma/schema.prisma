generator client {
  provider = "prisma-client-js"
}

model club {
  id           String @id @default(cuid())
  name         String @unique
  description  String
  president    String
  meeting_time String
  tags         tag[]
  user         user?   @relation(fields: [userId], references: [id])
  userId       String?

}

model tag {
  name   String @id @unique
  club   club?   @relation(fields: [clubId], references: [id])
  clubId String?
}

model user {
  id        String @id @default(cuid())
  firstname String
  lastname  String
  email     String @unique
  clubs     club[]
}
